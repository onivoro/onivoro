{
  "name": "server-twilio",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/server-twilio/src",
  "projectType": "library",
  "targets": {
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["libs/server-twilio/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/libs/server-twilio"],
      "options": {
        "jestConfig": "libs/server-twilio/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "build": {
      "executor": "@nrwl/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/server-twilio",
        "tsConfig": "libs/server-twilio/tsconfig.lib.json",
        "packageJson": "libs/server-twilio/package.json",
        "main": "libs/server-twilio/src/index.ts",
        "assets": ["libs/server-twilio/*.md"]
      }
    }
  },
  "tags": []
}
